  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "require": {
    "php": "^8.3",
    "symfony/console": "^7.1",
    "symfony/http-client": "^7.1",
    "symfony/dotenv": "^7.1",
    "symfony/cache": "^7.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.64",
    "symfony/var-dumper": "^7.1",
    "pestphp/pest": "^3.0",
    "mockery/mockery": "^1.6"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "pestphp/pest-plugin": true
    }
  }
}
